<Form.Input\n            type=\"password\"\n            placeholder=\"Password\"\n            value={password}\n            autoComplete=\"off\"\n            onChange={({ target }) => setPassword(target.value)}\n          />\n          <Form.Submit disabled={isInvalid} type=\"submit\">\n            Sign Up\n        </Form.Submit>\n\n          <Form.Text>\n            Already a user? <Form.Link to=\"/signin\">Sign in now.</Form.Link>\n          </Form.Text>\n          <Form.TextSmall>\n            This page is protected by Google reCAPTCHA to ensure you are not a\n            bot. Learn more.\n        </Form.TextSmall>\n        </Form.Base>\n      </Form>\n    </HeaderContainer>\n    <FooterContainer />\n  </>\n  );\n}\n","/Users/ntuclhub/Documents/netflixclone/netflix/src/components/header/index.js",[],"/Users/ntuclhub/Documents/netflixclone/netflix/src/components/header/styles/header.js",[],"/Users/ntuclhub/Documents/netflixclone/netflix/src/containers/header.js",[],"/Users/ntuclhub/Documents/netflixclone/netflix/src/components/feature/index.js",[],"/Users/ntuclhub/Documents/netflixclone/netflix/src/components/feature/styles/feature.js",[],"/Users/ntuclhub/Documents/netflixclone/netflix/src/lib/firebase.prod.js",["148"],"import Firebase from 'firebase/app';\nimport 'firebase/firestore';\nimport 'firebase/auth';\nimport { seedDatabase } from '../seed';\n\n// Need to seed the database \nconst config = {\n    apiKey: \"AIzaSyBtW8N7htdwDvKj6XxXMgW5_VUS-nCik64\",\n    authDomain: \"netflix-2ee25.firebaseapp.com\",\n    databaseURL: \"https://netflix-2ee25-default-rtdb.firebaseio.com\",\n    projectId: \"netflix-2ee25\",\n    storageBucket: \"netflix-2ee25.appspot.com\",\n    messagingSenderId: \"91203324326\",\n    appId: \"1:91203324326:web:031ae53829171d822a6767\"\n};\n\nconst firebase = Firebase.initializeApp(config);\n\n// seedDatabase(firebase);\n\nexport { firebase };\n","/Users/ntuclhub/Documents/netflixclone/netflix/src/seed.js",[],"/Users/ntuclhub/Documents/net